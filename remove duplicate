int k=1 ;
int i=1;
int j=0;

while(i<nums.size()){
    if(nums[i]==nums[i-1]){
        i++;
        continue;}
        
    else{
        nums[j]=nums[i-1];
            j++;
            i++;
            k++;
           
        }
       
    }
    


  return k;
     
    
